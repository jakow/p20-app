{
    "extends": ["plugin:flowtype/recommended", "airbnb"],
    "parser": "babel-eslint",
    "plugins": [
        "flowtype",
        "react",
        "import"
    ],
    "rules": {
        "global-require": "off",
        "react/jsx-filename-extension": "off",
        "no-underscore-dangle": "off",
        "no-mixed-operators": "off",
        "no-restricted-syntax": "off"
    }
}
